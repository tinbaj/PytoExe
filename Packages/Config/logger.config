[loggers]
keys=myAppLogger,root

[handlers]
keys=logfile,myAppLogger

[formatters]
keys=logFileFormatter,myAppLoggerFormatter

[logger_root]
level=DEBUG
handlers=logfile

[logger_myAppLogger]
level=DEBUG
handlers=myAppLogger
qualname=myAppLogger

[formatter_logFileFormatter]
format=%(message)s

[formatter_myAppLoggerFormatter]
format=%(asctime)s - %(name)s - %(levelname)s - %(message)s

[handler_logfile]
class=StreamHandler
level=ERROR
args=(sys.stdout,)
formatter=logFileFormatter

[handler_myAppLogger]
class=handlers.RotatingFileHandler
level=DEBUG
args=(r'C:\Users\user\PycharmProjects\FileParser\logs\myApp.log','a',5*1024*1024,5)
formatter=myAppLoggerFormatter
